{
  "name": "drawer",
  "description": "A small CSS3 and jQuery plugin for app style drawer menu.",
  "keywords": [
    "jQuery",
    "CSS",
    "drawer",
    "navigation",
    "responsive"
  ],
  "homepage": "http://git.blivesta.com/drawer",
  "main": [
    "dist/css/drawer.min.css",
    "dist/js/drawer.min.js"
  ],
  "ignore": [
    ".*"
  ],
  "dependencies": {
    "jquery": ">= 1.9.0",
    "iscroll": "~5.1.3",
    "bootstrap.dropdown": "~3.3.1"
  },
  "exportsOverride": {
    "iscroll": {
      "js": "build/iscroll.js"
    },
    "bootstrap.dropdown": {
      "js": "dropdown.js"
    }
  }
}
